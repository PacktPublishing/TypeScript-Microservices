{
  "name": "code-analyzer",
  "version": "1.0.0",
  "description": "Hello World,typescript microservices first chapter.",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.18.2",
    "config": "^1.28.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "routing-controllers": "^0.7.6",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.6.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/config": "0.0.33",
    "@types/cors": "^2.8.3",
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.39",
    "@types/node": "^8.0.58",
    "@types/winston": "^2.3.7",
    "auditjs": "^2.4.2"
  },
  "scripts": {
    "start": "npm run clean && npm run build && node ./dist/index.js",
    "clean": "node ./node_modules/rimraf/bin.js dist",
    "build": "node ./node_modules/typescript/bin/tsc",
    "audit": "auditjs",
    "analyze":"node ./node_modules/tslint/bin/tslint  -r node_modules/tslint-microsoft-contrib/ -c tslint.json -s src/**/*.ts",
    "fix":"node ./node_modules/tslint/bin/tslint  -r node_modules/tslint-microsoft-contrib/ -c tslint.json -s src/**/*.ts --fix"
  },
  "author": "parth ghiya",
  "license": "ISC"
}
